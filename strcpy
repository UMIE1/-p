#include<assert.h>//使用assert需要包含头文件assert.h
void my_strcpy(char*dis, char *sou){
	assert(dis!=NULL);//断言，判断（）的语句，如果后面的条件为假，则报错并找到错误的文件和地方
	assert(sou != NULL);
	while (*dis++ = *sou++)
	{
		;
	}
}
int main() {
	char arr[] = "##############";
	char arr1[] = "bit";
	my_strcpy(arr, arr1);
	printf("arr= %s", arr);
	return 0;
}
